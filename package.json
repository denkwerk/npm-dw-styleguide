{
  "name": "npm-dw-styleguide",
  "version": "2.0.3",
  "description": "automatically generate a static styleguide from html pattern",
  "repository": "https://github.com/denkwerk/npm-dw-styleguide.git",
  "main": "index.js",
  "keywords": [
    "pattern",
    "pattern-library",
    "styleguide",
    "living"
  ],
  "author": "Erik Marstaeller",
  "license": "MIT",
  "bin": {
    "dw-styleguide": "bin/dw-styleguide"
  },
  "scripts": {
    "lint": "eslint generate/** src/js/sg-scripts/** src/js/sg-scripts-prio/**",
    "lint-fix": "eslint generate/** src/js/sg-scripts/** src/js/sg-scripts-prio/*** --fix",
    "lint-styles": "stylelint sg-theme/**/*.scss --fix"
  },
  "dependencies": {
    "copy": "^0.3.0",
    "glob": "^7.1.0",
    "nopt": "^3.0.6",
    "nunjucks": "^2.5.2",
    "showdown": "^1.4.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "eslint": "^4.12.0",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^4.0.2",
    "gulp-livereload": "^3.8.1",
    "gulp-postcss": "^7.0.1",
    "gulp-sass": "^3.1.0",
    "gulp-sass-glob": "^1.0.8",
    "gulp-stylelint": "^6.0.0",
    "postcss": "^6.0.17",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^1.3.4"
  }
}
